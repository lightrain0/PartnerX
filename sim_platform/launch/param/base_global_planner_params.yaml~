
GlobalPlanner:
  #Specifies whether or not to allow the planner to create plans
  #that traverse unknown space
  allow_unknown: false
  #A tolerance on the goal point for the planner.
  default_tolerance: 0.8
  #whether or not to visualize the potential area computed via a PointCloud2
  visualize_potential: false
  #If true, use dijkstra's algorithm. Otherwise, A*.
  use_dijkstra: true
  #If true, use the quadratic approximation of the potential. Otherwise, use a simpler calculation.
  use_quadratic: true

  #If true, create a path that follows the grid boundaries. Otherwise, use a gradient descent method.
  use_grid_path: false

  old_navfn_behavior: false

NavfnROS:
  #that traverse unknown space
  allow_unknown: false
  #A tolerance on the goal point for the planner.
  default_tolerance: 0.8
  #whether or not to visualize the potential area computed via a PointCloud2
  visualize_potential: false
