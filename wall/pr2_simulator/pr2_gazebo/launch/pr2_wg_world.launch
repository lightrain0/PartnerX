  <arg name="throttled" default="false"/>
  -->

  <include file="$(find gazebo_ros)/launch/willowgarage_world.launch">
    <!--arg name="gui" value="$(arg gui)" /-->
    <!--arg name="paused" value="$(arg paused)" /-->
    <!-- TODO: throttled not implemented in gazebo_ros/empty_world.launch
    <arg name="throttled" value="$(arg throttled)" />
    -->
  </include>

  <!--arg name="world_file" default="simple_for_show"/-->

  <!-- Start Gazebo with an empty world -->
  <!--include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="use_sim_time" value="true"/>
    <arg name="debug" value="false"/>
    <arg name="world_name" value="$(find sim_platform)/worlds/$(arg world_file).world"/>
  </include-->

  <!-- start pr2 robot -->
  <include file="$(find pr2_gazebo)/launch/pr2.launch"/>

</launch>

